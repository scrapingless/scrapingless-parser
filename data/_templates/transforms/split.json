{
  "Split by separator and get First": {
    "example": "A|B|C > return A",
    "rule": "split",
    "splitSeparator": "|",
    "type": "getSingleIndex",
    "index": 1,
    "onerror": {
      "customVal": "",
      "type": "customVal"
    }
  },

  "Split by separator and get NTH": {    
    "example": "A|B|C > return C",
    "rule": "split",
    "splitSeparator": "|",
    "type": "getSingleIndex",
    "index": 3,
    "onerror": {
      "customVal": "",
      "type": "customVal"
    }
  },

  "Split by separator and join multiple values by index NTH": {
    "info": "Split text by separator and get first value",
    "infoExample": "A|B|C|D|E > return AC",
    "rule": "split",
    "splitSeparator": "|",
    "type": "joinMultipleIndexes",
    "multipleIndex": [1,3],
    "onerror": {
      "customVal": "",
      "type": "customVal"
    }
  }
}
