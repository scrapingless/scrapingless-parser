    "version": "0.2.0",
    "configurations": [       
        {
            "name": "DEBUG",
            "request": "launch",
            "cwd": "${workspaceFolder}/scrapingless-parser",
            "runtimeArgs": [
                "run-script",
                "nodemon"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Parser-API",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/scrapingless-api-parser/bin/www"
        },      
        
         
    ]
}